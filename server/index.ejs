<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Video Player</title>
  <style>
    #content {
      width: 100%;
      height: 100%;
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      display: flex;
      justify-content: center;
      align-items: center;
    }
  </style>
  <% if (debug) { %>
  <script src="<%= staticsBaseUrl %>video-player.bundle.js"></script>
  <% } else { %>
  <script src="<%= staticsBaseUrl %>video-player.bundle.min.js"></script>
  <% } %>
</head>
<body>

<div id="content">
  <div id="player">

  </div>
</div>
<script>
  var player = new VideoPlayer.Player({
    autoplay: false,
    muted: false,
    loop: false,
    preload: "metadata",
    volume: 0.5,
    size: {
      width: 760,
      height: 428
    },
    src: [
      {
      	url: 'https://vod-meta.wix.com/secure-files/eee8630aa5274e66813172fbeb2a742c?secure_data=c2F2ZV9hc19uYW1lPVdhbmRlcmx1c3Qmbm9uY2U9NDkxNzZiZjNhNGY1JmV4cGlyYXRpb25fdHM9MTQ5NzcyMzkzNSZmaWxlX3VybD0vYWVjYTA2MmMtZjUzNC00OTFmLWJjN2QtNTRmZjNlNWE5ZDE4L3ZpZGVvL2VlZTg2MzBhYTUyNzRlNjY4MTMxNzJmYmViMmE3NDJjL2Rhc2gvY2xpcC9maWxlLm1wZCZidWNrZXQ9dmlkZW8ud2l4c3RhdGljLmNvbQ==&secure_sig=d1d3c02d6795fc05ccf9712d4b4d5a5320e910f8',
        type: 'DASH'
      }
    ],
    overlay: true,
    screen: {
      indicateScreenClick: true
    }
  });
  document.getElementById('player').appendChild(player.node);
</script>
</body>
</html>
