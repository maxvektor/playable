<div
  data-hook="progress-control"
  class="{{=it.styles['seek-block']}}"
  tabindex="0">
  <div class="{{=it.styles['progress-bars-wrapper']}}">
    <div
      class="{{=it.styles['progress-bar']}} {{=it.styles['background']}}">
    </div>
    <div
      data-hook="progress-buffered"
      class="{{=it.styles['progress-bar']}} {{=it.styles['buffered']}}">
    </div>
    <div
      data-hook="progress-seek-to"
      class="{{=it.styles['progress-bar']}} {{=it.styles['seek-to']}}">
    </div>
    <div
      data-hook="progress-played"
      class="{{=it.styles['progress-bar']}} {{=it.styles['played']}}">
    </div>
    <div
      data-hook="progress-time-indicators"
      class="{{=it.styles['time-indicators']}}">
    </div>
  </div>
  <div
    data-hook="progress-hitbox"
    class="{{=it.styles['hitbox']}}">
  </div>
</div>
