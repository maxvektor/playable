<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Video Player</title>
    <style>
      #content {
        width: 625px;
        height: 351px;
      }
    </style>
    <% if (debug) { %>
      <script src="<%= staticsBaseUrl %>video-player.bundle.js"></script>
    <% } else { %>
      <script src="<%= staticsBaseUrl %>video-player.bundle.min.js"></script>
    <% } %>
</head>
<body>

<div id="content"></div>
<script>
    var player = new VideoPlayer({
      width: 700,
      height: 394,
      autoplay: false,
      muted: false,
      loop: true,
      nativeControls: false,
      preload: 'auto',
      poster: 'https://pbs.twimg.com/media/C3iThsgWcAAyXhm.jpg:large',

      src: [
        'https://clips.vorwaerts-gmbh.de/big_buck_bunny.mp4',
      ],

      overlay: true,
      controls: true,
      timeIndicator: true,
      progressControl: true,
      volumeControl: true,
      volume: 0.5,
      fullscreenControl: true,

      enableLogger: true
    });
    document.getElementById('content').appendChild(player.node);
</script>
</body>
</html>
